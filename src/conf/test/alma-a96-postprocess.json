{
  "isil": "DE-A96",
  "path": "src/test/resources/limetrans/input/",
  "postprocess": true,
  "alma": {
    "regexp": {
      "description": ".*vorhanden.*"
    }
  },
  "vars": {
    "external-maps": "/data/jenkins/limetrans/maps/test"
  },
  "input": {
    "queue": {
      "path": "${path}",
      "pattern": "alma.xml.gz"
    }
  },
  "output": {
    "json": "src/test/resources/limetrans/output%s/alma-a96-postprocess.json",
    "pretty-printing": true
  }
}
